# Variabel untuk docker-compose.yml
POSTGRES_DB=your_postgres_db
POSTGRES_USER=your_postgres_user
POSTGRES_PASSWORD=your_postgres_password

# API & Frontend URLs
# For local development:
NEXT_PUBLIC_API_URL=http://localhost:8080
ALLOWED_ORIGINS=http://localhost:3000

# For production with server IP (uncomment and update):
# NEXT_PUBLIC_API_URL=http://192.168.100.244:8080
# ALLOWED_ORIGINS=http://192.168.100.244:3000,http://localhost:3000

# For production with domain (uncomment and update):
# NEXT_PUBLIC_API_URL=https://seirotan.desa.id/api
# ALLOWED_ORIGINS=https://seirotan.desa.id,http://localhost:3000
